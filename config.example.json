{
	"baseUrl": "https://example.com",
	"contentSelector": "#main-content",
	"titleSelector": "h1",
	"maxDepth": 2,
	"excludeList": ["exclude-urls-that-include-this-string"],
	"retryOptions": {
 		"maxRetries": 7,
 		"initialDelay": 200
